@using WebStoryFroEveryting.Localizations
<html>
<head>
	<link href="~/css/reset.css" rel="stylesheet" />
	<link href="~/css/UnderwaterHunter/hunterForm.css" rel="stylesheet" />
	<link href="~/css/UnderwaterHunter/previewHunterCreate.css" rel="stylesheet" />
	<link href="~/css/UnderwaterHunter/specialStyles.css" rel="stylesheet" />
		
	<script src="https://cdnjs.cloudflare.com/ajax/libs/microsoft-signalr/6.0.1/signalr.js"></script>
	<script src="~/lib/jquery/dist/jquery.min.js"></script>
	@RenderSection("Scripts", false)
</head>
<body>	
	<div class="label-and-header">
		<div class="label">
			<img src="http://podvox.by/uploads/attachment/2014-03/1395307133_f16789d2c157.jpg" />
		</div>
		<div class="header">
			@HuntersStartPage.Key_UnderwaterHunterPage
		</div>
		<div class="buttonsMenu-links">
			<div class="text-Menu">Menu:</div>	
			<div class="mainPage">
				<a class="button-addNewHunter" href="/UnderwaterHunter/Index">
					@HuntersStartPage.Key_Menu
				</a>
			</div>
			<div class="newHunter">
				<a class="button-addNewHunter" href="/UnderwaterHunter/CreateNewHunter">
					Add new hunter</a>
			</div>	
			<div class="swapMeetProduct mainPage">
				<a class="button-swapMeetProduct" href="/SwapMeetProduct/Index">Swap meet</a>		
			</div>
		</div>
